{%- layout none -%}
{%- capture output -%}
    {%- for vendor in shop.vendors -%}
        {%- if output contains vendor -%}
        {%- else -%}
            "{{vendor}}"
            {%- unless forloop.last == true -%},{%- endunless -%}
        {%- endif -%}
    {%- endfor -%}
{%- endcapture -%}
{{ output | strip_newlines | prepend: '[' | append: ']' }}